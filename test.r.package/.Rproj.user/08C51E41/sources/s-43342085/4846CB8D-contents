testthat::test_that("Test functionality works as intended",
                    testthat::expect_equal(add_two_numbers(1,2), 3))

testthat::test_that("Check validation check works",{
                    testthat::expect_error(add_two_numbers("a", 2))
                    testthat::expect_error(add_two_numbers(1 ,"b"))})
